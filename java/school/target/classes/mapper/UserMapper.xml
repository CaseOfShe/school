<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.school.mapper.UserMapper">

    <resultMap id="BaseResultMap" type="com.school.entity.User">
            <id property="sid" column="sid" jdbcType="INTEGER"/>
            <result property="snumber" column="snumber" jdbcType="INTEGER"/>
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="school" column="school" jdbcType="VARCHAR"/>
            <result property="number" column="number" jdbcType="BIGINT"/>
            <result property="avatar" column="avatar" jdbcType="VARCHAR"/>
            <result property="phone" column="phone" jdbcType="VARCHAR"/>
            <result property="address" column="address" jdbcType="VARCHAR"/>
            <result property="birthday" column="birthday" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        sid,snumber,username,
        school,number,avatar,
        phone,address,birthday
    </sql>
</mapper>
